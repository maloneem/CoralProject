binner <- function(df, binWidth, delta) {
rowz = c()
for(j in 1:nrow(df)) {
col = c()
row_normed = as.numeric(df[j,-(1:3)])
for(i in seq(4, ncol(df)-binWidth, binWidth)){
end = i + binWidth - 1
col = c(col, sum(as.numeric(df[j,i:end])))
}
rowz = rbind(rowz, col)
}
return(rowz)
}
